{
  "name": "skifree",
  "version": "0.0.1",
  "description": "skifree made with javascript",
  "author": "J Lopes <mail@jlopes.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:offeringofpie/game-skifree.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.config.js",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [
    "skifree",
    "js",
    "canvas"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0-rc.0",
    "@babel/plugin-transform-spread": "^7.0.0-rc.0",
    "@babel/preset-env": "^7.0.0-rc.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.4",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.15.0-rc.13",
    "jest": "^22.4.4",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "preact": "^8.3.0",
    "preact-redux": "^2.0.3",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "rxjs": "^5.5.11",
    "style-loader": "^0.19.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.1.5",
    "webpack-hot-client": "^3.0.0",
    "webpack-notifier": "^1.6.0",
    "webpack-serve": "^1.0.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-spread",
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  },
  "dependencies": {}
}
