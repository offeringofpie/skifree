{
  "name": "skifree",
  "version": "0.0.1",
  "description": "skifree made with javascript",
  "author": "J Lopes <mail@jlopes.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:offeringofpie/game-skifree.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run dev:open && npm run dev:webpack",
    "dev:webpack": "./node_modules/webpack/bin/webpack.js",
    "dev:open": "open ./build/index.html",
    "git:clean": "rm -rf .git; git init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "log": "git log --abbrev-commit --date=short --pretty=format:'- %s Â· [%h](http://github.com/$3/$4/commit/%H) Â· ðŸ“… %ad' > CHANGELOG2.md && echo 'CHANGELOG.md udpated'"
  },
  "keywords": [
    "skifree",
    "js",
    "canvas"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-spread": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta.2",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.3",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "rxjs": "^5.5.5",
    "style-loader": "^0.19.0",
    "webpack": "^4.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-spread"
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  }
}
