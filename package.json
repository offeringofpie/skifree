{
  "name": "skifree",
  "version": "0.0.1",
  "description": "skifree made with javascript",
  "author": "J Lopes <mail@jlopes.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:offeringofpie/game-skifree.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "echo \"Error: no test specified\""
  },
  "keywords": [
    "skifree",
    "js",
    "canvas"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.44",
    "@babel/plugin-transform-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.2",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^0.15.0-rc.13",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.3",
    "preact": "^8.2.7",
    "preact-redux": "^2.0.3",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "rxjs": "^5.5.8",
    "style-loader": "^0.19.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-notifier": "^1.6.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "shippedProposals": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-spread", [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  }
}
